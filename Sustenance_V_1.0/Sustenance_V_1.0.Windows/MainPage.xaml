            <Border Margin="35,169,10,139" BorderBrush="Black" BorderThickness="1" CornerRadius="35" Background="#FFF1FFF8" Width="105" Height="460" HorizontalAlignment="Center" VerticalAlignment="Center">
                <Grid>
                    <Grid x:Name="City_g" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,326" PointerPressed="Ind_Grid_Clicked" Height="91">
