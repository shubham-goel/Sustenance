                        <Image x:Name="Manufacturing_Industry_i" Source="077-512.png" Width="80" Height="80" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                        <TextBlock x:Name="Manufacturing_Industry_n" HorizontalAlignment="Center" Height="35" Margin="0,30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="80" Foreground="White" FontSize="12" FontFamily="Segoe UI" Text="Industry" TextAlignment="Center" Padding="0,2,0,0"/>
                    </Grid>
                    <Grid x:Name="Agriculture_g" Margin="0,219,0,107" HorizontalAlignment="Center" VerticalAlignment="Center" PointerPressed="Ind_Grid_Clicked" Height="91">
                        <ProgressBar x:Name="Agriculture_Chart" Maximum="100" Value="30" Height="6" LargeChange="0" BorderBrush="{x:Null}" BorderThickness="1" Margin="0,82,0,-88" Background="#59A2A0A0" ScrollViewer.VerticalScrollBarVisibility="Disabled" Padding="1" FontFamily="Segoe UI" Width="80" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                        <Image x:Name="Agriculture_i" Source="Agriculture.png" Width="80" Height="80" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                        <TextBlock x:Name="Agriculture_n" HorizontalAlignment="Center" Height="18" Margin="0,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="60" Foreground="Black" FontSize="12" FontFamily="Segoe UI" Text="Agriculture" TextAlignment="Center" Padding="0,2,0,0"/>
                    </Grid>
                    <Grid x:Name="Health_g" Margin="0,327,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" PointerPressed="Ind_Grid_Clicked" Height="90">
