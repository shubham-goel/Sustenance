                        <Image x:Name="Health_i" Source="Health_icon_TF2.png" Width="80" Height="80" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                        <TextBlock x:Name="Health_n" HorizontalAlignment="Left" Height="18" Margin="10,29,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="60" Foreground="White" FontSize="12" FontFamily="Segoe UI" Text="Health" TextAlignment="Center" Padding="0,2,0,0"/>
                    </Grid>
                </Grid>
            </Border>
        </Grid>
        <Popup x:Name="message_popup" Margin="0,0,300,208" HorizontalAlignment="Center" VerticalAlignment="Center" IsLightDismissEnabled="True">
            <Grid Background="White" Height="200" Width="300">
                <Border HorizontalAlignment="Center" Height="200" VerticalAlignment="Center" Width="300" Background="#FFD8D8D8" CornerRadius="30" BorderBrush="#FF343434" BorderThickness="0,0,9,9" Margin="0" >
                    <TextBlock x:Name="message_popup_text" TextWrapping="Wrap" TextAlignment="Center" FontSize="20" FontFamily="Andalus" Height="82">
						<Run Text="Congratulations!"/>
						<LineBreak/>
						<Run Text="Message comes here!"/>
						<LineBreak/>
						<Run Text="Level Up!! "/>
                    </TextBlock>
                </Border>
            </Grid>
        </Popup>
    </Grid>
</Page>