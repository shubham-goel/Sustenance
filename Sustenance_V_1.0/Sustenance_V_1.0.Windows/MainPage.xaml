                        <TextBlock x:Name="City_n" HorizontalAlignment="Center" Height="18" Margin="13,30,7,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="60" Foreground="White" FontSize="12" FontFamily="Segoe UI" Text="City" TextAlignment="Center" Padding="0,2,0,0"/>
                    </Grid>
                    <Grid x:Name="Manufacturing_Industry_g" Margin="0,106,0,220" HorizontalAlignment="Center" VerticalAlignment="Center" PointerPressed="Ind_Grid_Clicked" Height="91">
